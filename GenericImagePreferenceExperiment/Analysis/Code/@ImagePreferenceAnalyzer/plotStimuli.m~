function plotStimuli(obj, whichDisplay)

    subplotPosVectors = NicePlot.getSubPlotPosVectors(...
                 'rowsNum',      obj.scenesNum, ...
                 'colsNum',      obj.scenesNum, ...
                 'widthMargin',  0.01, ...
                 'heightMargin', 0.01, ...
                 'leftMargin',   0.01, ...
                 'rightMargin',  0.01, ...
                 'bottomMargin', 0.01, ...
                 'topMargin',    0.01);
             
    figure(1);
    clf;
    
    for sceneIndex = 1:obj.scenesNum
        for toneMappingIndex = 1:obj.scenesNum
            stimIndex = conditionsData(sceneIndex, toneMappingIndex);
            imageHDR = squeeze(obj.thumbnailStimImages(stimIndex,1,:,:,:));
            imageLDR = squeeze(obj.thumbnailStimImages(stimIndex,2,:,:,:));
            subplot('Position', subplotPosVectors(toneMappingIndex,sceneIndex).v);
            imshow(imageHDR/255);
            drawnow;
        end
    end
    


end

