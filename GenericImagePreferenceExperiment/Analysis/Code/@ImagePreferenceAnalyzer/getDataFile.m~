function getDataFile(obj,rootDir)
rootDir
    cd(rootDir);
    pwd
    cd ..
    cd ..
    dataDir = fullfile(pwd, 'Data');
    dataDir
    
    [fileName,pathName] = uigetfile({'*.mat'},'Select a data file for analysis', dataDir);
    obj.dataFile = fullfile(pathName, fileName);
    cd(rootDir);
    
end